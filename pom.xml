				<groupId>org.apache.mina</groupId>
				<artifactId>mina-core</artifactId>
				<version>${mina}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
	<properties>
		<java>1.8</java>
		<hibernate>5.0.11.Final</hibernate>
		<validation.api>1.1.0.Final</validation.api>
		<spring.cloud>1.2.1.RELEASE</spring.cloud>
		<mysql>6.0.5</mysql>
		<activemq>5.14.1</activemq>
		<spring.boot>1.4.2.RELEASE</spring.boot>
		<spring.data.commons>1.12.5.RELEASE</spring.data.commons>
		<apache.dbcp2>2.1.1</apache.dbcp2>
		<javax.transaction>1.2</javax.transaction>
		<spring.framework>4.3.4.RELEASE</spring.framework>
		<spring.security>4.1.3.RELEASE</spring.security>
		<spring.security.oauth>2.0.12.RELEASE</spring.security.oauth>
		<apache.httpclient>4.5.2</apache.httpclient>
		<commons-fileupload>1.3.2</commons-fileupload>
		<h2database>1.4.193</h2database>
		<jackson>2.8.4</jackson>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<mina>3.0.0-M2</mina>
	</properties>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>${java}</source>
					<target>${java}</target>
					<encoding>${project.build.sourceEncoding}</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>3.0.1</version>
				<configuration>
					<delimiters>
						<delimiter>@</delimiter>
					</delimiters>
					<useDefaultDelimiters>false</useDefaultDelimiters>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>3.0.1</version>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar-no-fork</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
	</build>
	<modules>
		<module>cloud</module>
		<module>framework</module>


	</modules>
	<profiles>
		<profile>
			<id>dev-home</id>
			<properties>
				<profileActive>dev-home</profileActive>
			</properties>
		</profile>
		<profile>
			<id>dev-work</id>
			<properties>
				<profileActive>dev-work</profileActive>
			</properties>
		</profile>
		<profile>
			<id>dev</id>
			<properties>
				<profileActive>dev</profileActive>
			</properties>
		</profile>
		<profile>
			<id>qa</id>
			<properties>
				<profileActive>qa</profileActive>
			</properties>
		</profile>
		<profile>
			<id>uat</id>
			<properties>
				<profileActive>uat</profileActive>
			</properties>
		</profile>
		<profile>
			<id>prod</id>
			<properties>
				<profileActive>prod</profileActive>
			</properties>
		</profile>
	</profiles>
</project>